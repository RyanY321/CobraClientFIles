<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <runtime>
    <ThrowUnobservedTaskExceptions enabled="true" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-13.0.0.0" newVersion="13.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <!-- Only uncomment this on dev/test machines -->
  <!--<system.diagnostics>
    <switches>
      <add name="System.Net" value="Warning" />
      <add name="System.Net.Sockets" value="Warning" />
      <add name="System.Net.Http" value="Warning" />
      <add name="System.Net.Cache" value="Warning" />
    </switches>
  </system.diagnostics>-->
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true">
    <include file="cmwsrv-nlog-*.config" ignoreErrors="true" />
    <variable name="xcptLayout" value="${onexception:inner=${newline}${exception:format=ToString}" />
    <variable name="levelFc" value="${level:format=FirstCharacter}" />
    <variable name="shortLogger" value="${logger:shortName=true}" />
    <variable name="threadLayout" value="${pad:padding=2:padCharacter=0:inner=${threadid}}" />
    <variable name="propsLayout" value="${all-event-properties}" />
    <variable name="appLayout" value="${time}|${levelFc}|[${threadLayout}]|${shortLogger}|${message}${propsLayout}${xcptLayout}" />
    <variable name="sysLayout" value="${time}|${levelFc}|[${threadLayout}]|${pad:padding=-4:fixedLength=true:inner=${shortLogger}} ${message}" />
    <variable name="evtLayout" value="${time}|${message}${propsLayout}${xcptLayout}" />
    <variable name="commonLogs" value="${specialfolder:folder=CommonApplicationData}\${gdc:item=product}\system\logs" />
    <variable name="fx" value="${commonLogs}\hsssvc.${assembly-version}.${shortdate}" />
    <variable name="fx-events" value="${commonLogs}\hsssvc.events.${assembly-version}.${shortdate}" />
    <variable name="fx-nc" value="${commonLogs}\hsssvc.notificationchannel.${assembly-version}.${shortdate}" />
    <variable name="maxFileSize" value="209715200" />
    <targets async="true">
      <target name="app" xsi:type="SplitGroup">
        <target xsi:type="ColoredConsole" layout="${appLayout}" useDefaultRowHighlightingRules="true" detectConsoleAvailable="true" />
        <target xsi:type="File" encoding="utf-8" layout="${appLayout}" fileName="${fx}.log" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true" />
        <target xsi:type="File" encoding="utf-8" fileName="${fx}.jlog" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true">
          <layout xsi:type="JsonLayout" includeAllProperties="true">
            <attribute name="time" layout="${longdate}" />
            <attribute name="level" layout="${level:upperCase=true}" />
            <attribute name="thread" layout="${threadName}" />
            <attribute name="threadId" layout="${threadId}" />
            <attribute name="logger" layout="${logger}" />
            <attribute name="user" layout="${windows-identity}" />
            <!-- <attribute name="identity" layout="${identity}" /> -->
            <!-- <attribute name="host" layout="${machinename}" /> -->
            <!-- <attribute name="class" layout="${callsite:classname=true:includesourcepath=false:methodName=false}" /> -->
            <attribute name="method" layout="${callsite:classname=false:includesourcepath=false:methodName=true}" />
            <!-- <attribute name="file" layout="${callsite:classname=false:filename=true:includesourcepath=true:methodName=false}" /> -->
            <!-- <attribute name="line" layout="${callsite-linenumber}" /> -->
            <attribute name="domain" layout="${processname}" />
            <attribute name="processId" layout="${processid}" />
            <attribute name="message" layout="${message}" />
            <attribute name="exception" layout="${exception:format=ToString}" />
            <!-- <attribute name="ndc" layout="${ndc}" /> -->
            <!-- <attribute name="mdc" layout="${mdc:item=clientid}" /> -->
          </layout>
        </target>
      </target>
      <target name="sys" xsi:type="SplitGroup">
        <target xsi:type="ColoredConsole" layout="${sysLayout}" useDefaultRowHighlightingRules="true" detectConsoleAvailable="true" />
        <target xsi:type="File" encoding="utf-8" layout="${sysLayout}" fileName="${fx}.log" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true" />
        <target xsi:type="File" encoding="utf-8" fileName="${fx}.jlog" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true">
          <layout xsi:type="JsonLayout" includeAllProperties="true">
            <attribute name="time" layout="${longdate}" />
            <attribute name="level" layout="${level:upperCase=true}" />
            <attribute name="thread" layout="${threadName}" />
            <attribute name="threadId" layout="${threadId}" />
            <attribute name="logger" layout="${logger}" />
            <attribute name="user" layout="${windows-identity}" />
            <!-- <attribute name="identity" layout="${identity}" /> -->
            <!-- <attribute name="host" layout="${machinename}" /> -->
            <!-- <attribute name="class" layout="${callsite:classname=true:includesourcepath=false:methodName=false}" /> -->
            <attribute name="method" layout="${callsite:classname=false:includesourcepath=false:methodName=true}" />
            <!-- <attribute name="file" layout="${callsite:classname=false:filename=true:includesourcepath=true:methodName=false}" /> -->
            <!-- <attribute name="line" layout="${callsite-linenumber}" /> -->
            <attribute name="domain" layout="${processname}" />
            <attribute name="processId" layout="${processid}" />
            <attribute name="message" layout="${message}" />
            <attribute name="exception" layout="${exception:format=ToString}" />
            <!-- <attribute name="ndc" layout="${ndc}" /> -->
            <!-- <attribute name="mdc" layout="${mdc:item=clientid}" /> -->
          </layout>
        </target>
      </target>
      <target name="events" xsi:type="SplitGroup">
        <target xsi:type="ColoredConsole" layout="${evtLayout}" useDefaultRowHighlightingRules="true" detectConsoleAvailable="true" />
        <target xsi:type="File" encoding="utf-8" layout="${evtLayout}" fileName="${fx-events}.log" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="false" keepFileOpen="true" />
        <target xsi:type="File" encoding="utf-8" fileName="${fx-events}.jlog" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true">
          <layout xsi:type="JsonLayout" includeAllProperties="true">
            <attribute name="time" layout="${longdate}" />
            <attribute name="level" layout="${level:upperCase=true}" />
            <attribute name="thread" layout="${threadName}" />
            <attribute name="threadId" layout="${threadId}" />
            <attribute name="logger" layout="${logger}" />
            <attribute name="user" layout="${windows-identity}" />
            <!-- <attribute name="identity" layout="${identity}" /> -->
            <!-- <attribute name="host" layout="${machinename}" /> -->
            <!-- <attribute name="class" layout="${callsite:classname=true:includesourcepath=false:methodName=false}" /> -->
            <attribute name="method" layout="${callsite:classname=false:includesourcepath=false:methodName=true}" />
            <!-- <attribute name="file" layout="${callsite:classname=false:filename=true:includesourcepath=true:methodName=false}" /> -->
            <!-- <attribute name="line" layout="${callsite-linenumber}" /> -->
            <attribute name="domain" layout="${processname}" />
            <attribute name="processId" layout="${processid}" />
            <attribute name="message" layout="${message}" />
            <attribute name="exception" layout="${exception:format=ToString}" />
            <!-- <attribute name="ndc" layout="${ndc}" /> -->
            <!-- <attribute name="mdc" layout="${mdc:item=clientid}" /> -->
          </layout>
        </target>
      </target>
      <target name="appnc" xsi:type="SplitGroup">
        <target xsi:type="ColoredConsole" layout="${appLayout}" useDefaultRowHighlightingRules="true" detectConsoleAvailable="true" />
        <target xsi:type="File" encoding="utf-8" layout="${appLayout}" fileName="${fx-nc}.log" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true" />
        <target xsi:type="File" encoding="utf-8" fileName="${fx-nc}.jlog" maxArchiveFiles="1" archiveAboveSize="${maxFileSize}" archiveEvery="Day" concurrentWrites="true">
          <layout xsi:type="JsonLayout" includeAllProperties="true">
            <attribute name="time" layout="${longdate}" />
            <attribute name="level" layout="${level:upperCase=true}" />
            <attribute name="thread" layout="${threadName}" />
            <attribute name="threadId" layout="${threadId}" />
            <attribute name="logger" layout="${logger}" />
            <attribute name="user" layout="${windows-identity}" />
            <!-- <attribute name="identity" layout="${identity}" /> -->
            <!-- <attribute name="host" layout="${machinename}" /> -->
            <!-- <attribute name="class" layout="${callsite:classname=true:includesourcepath=false:methodName=false}" /> -->
            <attribute name="method" layout="${callsite:classname=false:includesourcepath=false:methodName=true}" />
            <!-- <attribute name="file" layout="${callsite:classname=false:filename=true:includesourcepath=true:methodName=false}" /> -->
            <!-- <attribute name="line" layout="${callsite-linenumber}" /> -->
            <attribute name="domain" layout="${processname}" />
            <attribute name="processId" layout="${processid}" />
            <attribute name="message" layout="${message}" />
            <attribute name="exception" layout="${exception:format=ToString}" />
            <!-- <attribute name="ndc" layout="${ndc}" /> -->
            <!-- <attribute name="mdc" layout="${mdc:item=clientid}" /> -->
          </layout>
        </target>
      </target>
    </targets>
    <rules>
      <logger name="*.NotificationChannel.*" level="Off" writeTo="appnc" final="false" />
      <logger name="Foundation.Tracking.Distribution.*" level="Off" writeTo="events" final="false" />
      <logger name="*.GprEventTracker" level="Off" writeTo="events" final="false" />
      <logger name="Foundation.Analytics.GA.PropertyTracker" level="Off" writeTo="events" final="false" />
      <logger name="System.*" minlevel="Warn" writeTo="sys" final="true" />
      <logger name="Hss.*" minlevel="Warn" writeTo="app" final="true" />
      <logger name="Foundation.*" minlevel="Warn" writeTo="app" final="true" />
      <logger name="*" minlevel="Warn" writeTo="app" final="true" />
    </rules>
  </nlog>
</configuration>